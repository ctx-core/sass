{
	"name": "@ctx-core/sass",
	"version": "12.1.18",
	"description": "ctx-core sass",
	"keywords": [
		"ctx-core",
		"svelte",
		"sass",
		"postcss"
	],
	"homepage": "https://github.com/ctx-core/sass#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sass/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sass.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.1.0",
		"@ctx-core/string": "workspace:^9.0.39",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.123",
		"cheerio": "1.0.0-rc.9",
		"dom-serializer": "^1.3.2",
		"domelementtype": "2.2.0",
		"domhandler": "^4.3.0",
		"entities": "^3.0.1",
		"htmlparser2": "^7.2.0",
		"node-sass-package-importer": "^5.3.2",
		"postcss": "^7.0.39",
		"sass": "^1.45.1"
	},
	"devDependencies": {
		"@types/sass": "^1.43.1",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"noUpdate": [
		"postcss"
	],
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}