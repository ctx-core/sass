{
	"name": "@ctx-core/sass",
	"version": "8.1.4",
	"description": "ctx-core sass",
	"keywords": [
		"ctx-core",
		"svelte",
		"sass",
		"postcss"
	],
	"homepage": "https://github.com/ctx-core/sass#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sass/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sass/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./index.js",
	"module": "./index.js",
	"types": "./index.ts",
	"bin": {
		"npm-rebuild-node-sass.sh": "bin/npm-rebuild-node-sass.sh"
	},
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo",
		"compile": "./node_modules/.bin/tsc -b",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^19.1.15",
		"@ctx-core/string": "workspace:^5.0.5",
		"cheerio": "^1.0.0-rc.5",
		"domelementtype": "2.1.0",
		"domhandler": "^4.0.0",
		"domutils": "^2.4.4",
		"htmlparser2": "^6.0.0",
		"node-sass": "^5.0.0",
		"node-sass-package-importer": "^5.3.2",
		"postcss": "^7.0.35"
	},
	"devDependencies": {
		"@types/sass": "^1.16.0",
		"typescript": "^4.1.3"
	},
	"noUpdate": [
		"postcss"
	],
	"publishConfig": {
		"access": "public"
	}
}
