{
	"name": "@ctx-core/sass",
	"version": "12.0.30",
	"description": "ctx-core sass",
	"keywords": [
		"ctx-core",
		"svelte",
		"sass",
		"postcss"
	],
	"homepage": "https://github.com/ctx-core/sass#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sass/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sass/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.21",
		"@ctx-core/string": "workspace:^9.0.19",
		"cheerio": "1.0.0-rc.9",
		"dom-serializer": "^1.3.2",
		"domelementtype": "2.2.0",
		"domhandler": "^4.2.2",
		"entities": "^3.0.1",
		"esm": "^3.2.25",
		"htmlparser2": "^7.1.2",
		"node-sass-package-importer": "^5.3.2",
		"postcss": "^7.0.36",
		"sass": "^1.39.2"
	},
	"devDependencies": {
		"@types/sass": "^1.16.1",
		"typescript": "^4.4.3"
	},
	"noUpdate": [
		"postcss"
	],
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}