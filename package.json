{
	"name": "@ctx-core/sass",
	"version": "11.0.2",
	"description": "ctx-core sass",
	"keywords": [
		"ctx-core",
		"svelte",
		"sass",
		"postcss"
	],
	"homepage": "https://github.com/ctx-core/sass#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sass/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sass/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.0.0",
		"@ctx-core/string": "workspace:^8.0.0",
		"cheerio": "1.0.0-rc.9",
		"dom-serializer": "^1.3.2",
		"domelementtype": "2.2.0",
		"domhandler": "^4.2.0",
		"entities": "^2.2.0",
		"htmlparser2": "^6.1.0",
		"node-sass-package-importer": "^5.3.2",
		"postcss": "^7.0.36",
		"sass": "^1.35.0"
	},
	"devDependencies": {
		"@types/sass": "^1.16.0",
		"typescript": "^4.3.2"
	},
	"noUpdate": [
		"postcss"
	],
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}