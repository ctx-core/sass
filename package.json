{
	"name": "@ctx-core/sass",
	"version": "12.3.180",
	"description": "ctx-core sass",
	"keywords": [
		"ctx-core",
		"svelte",
		"sass",
		"postcss"
	],
	"homepage": "https://github.com/ctx-core/sass#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/sass/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sass.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-sass@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./globalize",
		"./render_sass",
		"./sass_markup_",
		"./sass_preprocess_",
		"./sass_style",
		"./sass_style_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"css-select": "^5.1.0",
		"ctx-core": "^7.1.3",
		"dom-serializer": "^2.0.0",
		"domelementtype": "2.3.0",
		"domhandler": "^5.0.3",
		"entities": "^4.5.0",
		"htmlparser2": "^9.1.0",
		"node-sass-package-importer": "^5.3.3",
		"postcss": "^8.4.38",
		"sass": "^1.74.1"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"noUpdate": [],
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}